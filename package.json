{
    "name": "simplestate",
    "version": "0.0.1",
    "description": "A React App Simple State managment system",
    "main": "src/index.js",
    "engines": {
        "node": ">=16.13.2",
        "npm": ">=8.1.0"
    },
    "scripts": {
        "preinstall": "npx check-engine",
        "fix-lint-staged": "lint-staged",
        "prepare": "husky install",
        "check-engines": "check-engine",
        "build": "webpack --config src/webpack.config.js --mode production",
        "lint": "eslint --ext .jsx --ext .js --ext .json src",
        "fix-lint": "eslint --fix --ext .jsx --ext .js --ext .json src",
        "jsinspect": "jsinspect src",
        "test": "nyc mocha \"./src/**/*.test.{js,ts,jsx,tsx}\" --exit"
    },
    "postInstall": "npm run prepare",
    "lint-staged": {
        "*.{js,jsx,json,ts,tsx}": [
            "eslint --fix --ext .jsx --ext .js --ext .ts --ext .tsx --ext .json"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CoMfUcIoS/simpleState.git"
    },
    "author": "Ioannis Karasavvaidis (ioannis@studio110.eu)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/CoMfUcIoS/simpleState/issues"
    },
    "homepage": "https://github.com/CoMfUcIoS/simpleState#readme",
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/node": "^7.16.8",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.16.11",
        "@babel/register": "^7.17.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
        "babel-plugin-module-resolver": "^4.1.0",
        "chai": "^4.3.6",
        "check-engine": "^1.10.1",
        "enzyme": "^3.11.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-import-helpers": "^1.2.1",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.3",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^7.0.4",
        "jsdom": "^19.0.0",
        "jsinspect": "github:Ioannis-Karasavvaidis/jsinspect",
        "lint-staged": "^12.3.5",
        "mocha": "^9.2.1",
        "nyc": "^15.1.0",
        "prettier": "^2.5.1",
        "sinon-chai": "^3.7.0",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2"
    },
    "dependencies": {
        "react": "^17.0.2"
    }
}
